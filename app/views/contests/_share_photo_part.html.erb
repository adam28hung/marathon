<div class="col-lg-12" id='photos_container'>
  <div class="col-lg-4"></div>
  <div class="col-lg-4 thumb">
    <%= link_to @photo[0]['pageURL'], target: '_blank', title: @photo[0]['objectId'], class: 'thumbnail' do %>
      <%= image_tag(@photo[0]['imageURL'], class: "img-responsive", alt: "#{@photo[0]['objectId']}" )%>
    <% end %> 
    <p><%= link_to "圖片來源：#{@photo[0]['owner']}", @photo[0]['pageURL'], target: '_blank', title: show_share_title %></p>
  </div>
  <div class="col-lg-4"></div>
</div>